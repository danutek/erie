#remoteDefaultQueueManager: '{MQConnectionPolicy}:TLSClient'
ResourceManagers:
  JVM:
     jvmMinHeapSize: 33554432    # minimum JVM  heap size in bytes
     jvmMaxHeapSize: 268435456   # maximum JVM heap size in bytes
     jvmDebugPort: 9175
# for more heap use settings below
#    jvmMinHeapSize: 262144000        # minimum JVM  heap size in bytes
#    jvmMaxHeapSize: 1073741824       # maximum JVM heap size in bytes
     keystoreType: 'JKS'                # JVM key store type
     keystoreFile: '/home/aceuser/keystores/truststore.jks' # JVM location of the key store
     keystorePass: 'truststore::truststorePass'  # JVM resource alias containing the key store password
     truststoreType: 'JKS'               # JVM trust store type
     truststoreFile: '/home/aceuser/keystores/truststore.jks'   # JVM location of the trust store
     truststorePass: 'truststore::truststorePass'  # JVM resource alias containing the trust store password
#UserVariables:
#  ExampleUDPKey: 'udp-value'
